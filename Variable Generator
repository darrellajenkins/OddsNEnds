import random

# This is a random variable generator.  Makes 260 but can always change the multiplier to expand database for greater needs.  
# This provides variables to work in concert with user input, as in the user dictionary generator.

alpha = ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 's', 'y', 'z']
alphas = [random.choice(alpha) + random.choice(alpha) for a in alpha*10]
print(alphas)
print(len(alphas))

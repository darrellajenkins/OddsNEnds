import string

import itertools

# This program will produce every possible variable with 4 alpha chararacters (456,976), stores them in a list as dictionaries.

letters = string.ascii_lowercase 

sample = [''.join(p) for p in itertools.product(letters, repeat=4)]

for sam in sample:
    globals()[sam] = {}

aaaa['Artist'] = 'Prince' 
aaab['Artist'] = 'Madonna'
aaac['Artist'] = 'Whitney Houston' 
aaad['Artist'] = 'Cannons'

print(aaaa)
print(aaab)
print(aaac)
print(aaad)
print(len(sample))

print(sample[0:104])

s = input("Which name would you like to check? ")
if s in sample:  
    print(f"\033[32m{s.title()} \033[39m has been located in the data stream as item \033[32mnumber {(sample.index(s)+1)}\033[39m.\n")
elif s not in sample:
    print(f"A \033[31m {s} \033[39m was not found in this data set at all.")
